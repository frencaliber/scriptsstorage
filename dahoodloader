local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("LuaWare Da Hood Loader [2] Scripts", "Synapse")

local Tab = Window:NewTab("Loader")
local LoaderSection = Tab:NewSection("LuaWare.CC is for locking, silent aim and etc")
local LoaderSection = Tab:NewSection("LuaWare Z is for other stuff like main stuff")

LoaderSection:NewButton("LuaWare.CC", "Executes luaware.cc", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/frencaliber/scriptsstorage/main/luaware.cc",true))()
end)
LoaderSection:NewLabel("Recommended")

LoaderSection:NewButton("LuaWare Z", "Executes luaware z", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/frencaliber/scriptsstorage/main/luaware%20z",true))()
end)

Section:NewLabel("Credits to 50CalSpecialist#0001")
Section:NewLabel("2140 Dev")
Section:NewLabel("Aim_exe Dev")
Section:NewLabel("LuaWare Softwares")
